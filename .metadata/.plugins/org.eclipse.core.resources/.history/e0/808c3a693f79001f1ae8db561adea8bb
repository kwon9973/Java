package data;


public class Data {
	
	private double [][] var;
	private int label = 100;
	private int size = 2;
	
	
	public Data() {
		var = new double [label][size];
	}
	public Data(int label) {
		var = new double [label][size]; 
	}
	public void setSize(int label) {
		var = new double[label][this.size];
	}
	public double [][] getData() {
		 return var;
	}
	public void setData(int label, int size, double value) {
		var[label][size] = value;
	}
	public void displayData() {
		for (int i = 0; i < this.label; i++) {
			for (int j = 0; j < this.size; j++) {
				System.out.print(this.var[i][j]);	
			}
		}

	}
	

}
